<svg width="480" height="133" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <foreignObject width="480" height="133">
        <div xmlns="http://www.w3.org/1999/xhtml" class="container">
            <style>

                div {
                    font-family: Roboto;
                }

                .main {
                    border-style: solid;
                    border-width: 1px;
                    border-right: none;
                    border-color:#c4302b ;
                    display: flex;
                    margin-top: 1px;
                    width:99%;
                    padding: 10px 0px 7px 0px;
                    border-radius: 60px 25px 25px 60px;
                    position: relative;


                }

                .container {
                    margin-left:5px;
                    padding: 5px 0px 5px 0px;
                }


                .art {
                    float: left;
                    width: 27%;
                    margin-left: -1px;
                }

                .text {
                    width: 72%;
                }

                .channel-name {
                    font-size: 25px;
                    color: #666;

                    text-align: left;
                    margin-top: 3px;
                }

                .channel-info {
                    float: left !important;
                    width: 90%;
                }

                .channel-subscriber {
                    font-size: 13px;
                    color: #b3b3b3;
                    text-align: center;
                    margin-bottom: 5px;
                    float: left !important;
                }

                .album-cover {
                    border-radius: 50%;
                    height: 100px;
                    width: 100px;
                    -webkit-box-shadow: 0px 0px 2px 0px rgba(107,107,107,1);
                    -moz-box-shadow: 0px 0px 2px 0px rgba(107,107,107,1);
                    box-shadow: 0px 0px 2px 0px rgba(107,107,107,1);
                }

                .ribbon {
                    position: absolute;
                    right: -5px; top: -1px;
                    z-index: 1;
                    overflow: hidden;
                    width: 500px !important;
                    height: 500px !important;
                    text-align: right;
                    border-width: 1px;
                    border-right: none;
                    border-color:#c4302b;

                }
                .ribbon span {
                    border-radius: 15px 15px 0px 0px;
                     padding-left: 11px;
                     padding-right: 12px;
                     font-size: 11px;
                     font-weight: bold;
                     color: #FFF;
                     text-transform: uppercase;
                     text-align: center;
                     line-height: 20px;
                     transform: rotate(90deg);
                     -webkit-transform: rotate(90deg);
                     width: 100px;
                     display: block;
                     background: #c4302b ;
                     background: linear-gradient(#c4302b 0%, #c4302b 100%);
                     border-style: solid;
                     border-width: 1px;
                     border-right: none;
                     border-color:#c4302b;
                     position: absolute;
                     top: 50px; right: -45px;

                }
                .ribbon span::before {
                      content: "";
                      position: absolute; left: 0px; top: 100%;
                      z-index: -1;
                      border-color:red;
                      border-left: 3px solid #c4302b;
                      border-right: 3px solid transparent;
                      border-bottom: 3px solid transparent;
                      border-top: 3px solid #c4302b;
                }
                .ribbon span::after {
                  content: "";
                  position: absolute; right: 0px; top: 100%;
                  z-index: -1;
                  border-left: 3px solid transparent;
                  border-right: 3px solid #c4302b;
                  border-bottom: 3px solid transparent;
                  border-top: 3px solid #c4302b;
                  margin-top:-1px;
                  margin-right:0.050em !important;
                }

                .youtube-logo {
                    width:52%;
                    height: 52%;
                    margin-bottom:-3px;
                    margin-right: 1px;
                }

               .channel-other-info {
                    float: left !important;
                    width: 100%;
                    margin-top:5px;
               }

               .channel-other-value {
                    font-size: 15px;
                    color: #666;
                    text-align: left;
                    margin-top: 3px;
               }

               .channel-other-label {
                    font-size: 9px;
                    color: #666;
                    text-align: center;
                    margin-bottom: 5px;
                    float: left !important;
               }

               li {
                    float: left;
                    list-style-type: none;
                    margin-right:15px;
               }

               .info-logos {
                    height: 18px;
                    width: 18px;
                    vertical-align: baseline ;
                    margin: -2px;
               }
                .verified-badge {
                    width: 6%;
                    height:6%;
                }
            </style>

            {% if channelName %}

            <div class="main">
                 <div class="ribbon">
                    <span>
                        <img src="data:image/png;base64, {{youtubeIcon}}" class="youtube-logo"/>
                    </span>
                </div>
                <a class="art" href="{}" target="_BLANK">
                    <center>
                            <img src="data:image/png;base64, {{channelLogo}}" class="album-cover" />
                    </center>
                </a>
                <div class="text">
                    <div class="channel-info">
                        <div class="channel-name">{{channelName}}  <img src="data:image/png;base64, {{verifiedBadge}}" class="verified-badge" /></div>
                        <div class="channel-subscriber">{{channelSubscriberCount}} subscribers</div>
                    </div>
                    <div class="channel-other-info">
                        <li>
                            <div class="channel-other-value">
                             <img src="data:image/png;base64, {{channelCountryLogo}}" class="info-logos" />
                            {{channelCountry}}
                            </div>
                            <div class="channel-other-label">Country</div>
                        </li>
                        <li>
                            <div class="channel-other-value">
                             <img src="data:image/png;base64, {{channelVideoLogo}}" class="info-logos" />
                            {{channelVideoCount}}
                            </div>
                            <div class="channel-other-label">Upload Videos</div>
                        </li>
                        <li>
                            <div class="channel-other-value">
                             <img src="data:image/png;base64, {{channelViewLogo}}" class="info-logos" />
                            {{channelViewCounts}}
                            </div>
                            <div class="channel-other-label">Total Views</div>
                        </li>
                    </div>

                </div>

                {% else %}
                    <div class="No Youtube Status">No Youtube Status</div>
                {% endif %}
            </div>

        </div>
    </foreignObject>
</svg>